         {\refstepcounter{figure}%
          \def\captiontag{\figurename \ \thefigure.}% 
         \warpHTMLonly{
        <div id="fig\thefigure" class="figure" style="text-align:center">
\addtocontents{toc}{<span class="tOcMisc">
         <span id="tocFig\thefigure" onclick="LinkTo('fig\thefigure')">
         Figure \thefigure</span> 
         </span>}
}           
{
\warpHTMLonly{
</div>
}
}
}


\def\tablename{Table}
\setcounter{table}0
\def\thetable{\arabic{table}} 

\renewenvironment{table}
         {\refstepcounter{table}%
          \def\captiontag{\tablename \ \thetable.}% 
         \warpHTMLonly{
        <div id="table\thetable" class="table" style="text-align:center">
\addtocontents{toc}{<span class="tOcMisc">
         <span id="tocTable\thetable" onclick="LinkTo('Table\thetable')">
         Table \thetable</span> 
         </span>}
}           
{
\warpHTMLonly{
</div>
}
}
}


\def\caption#1{
\warpHTMLonly{
<br><span class="caption">
<span class="TagCaption">\captiontag</span> #1</span>
}
}

\def\lpict#1{<div class="lpict">{#1}</div>}
\def\rpict#1{<div class="rpict">{#1}</div>}

\def\icon{\@ifnextchar[{\op@icon}{\noop@icon}}
\def\noop@icon#1{
<img src="#1" class="icon">}
\def\op@icon[#1=#2]#3{
<img src="#3" style="#1:#2" class="icon">}

\newcommand{\url}[2]{
<a href="{#1}">{#2}</a>}

\newcounter{container}
\def\thecontainer{\arabic{container}}
\setcounter{container}\z@


\newenvironment{container}[1]{
\refstepcounter{container}%
<div id="#1" class="container\thecontainer">}
{</div>}

\newenvironment{fContainer}[1]{
\refstepcounter{container}%
<div id="#1" class="fcontainer\thecontainer" style="text-align:center">}
{</div>}


\newenvironment{acknowledgements}{
\pre@section
<div class="Acknowledgements">
<h6 class="Title TitleAcknowledgements">
<span onclick="ReverseDisplay('bOdYackn')">Acknowledgements. </span></h6
><span id="bOdYackn" class="bOdY hide">}
{</span></div>}

\def\qed{<span><img src="icons/square.png" width=14 height=14
style="float:right"></span>}

\def\floatnote#1#2{
<span onclick="Cp2Recto('#2')" class="cp2recto">#1</span>}


\makeatother
